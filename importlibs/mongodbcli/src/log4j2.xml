<?xml version="1.0" encoding="UTF-8"?>  
  
<Configuration status="OFF">  
    <Appenders>  
        <Console name="Console" target="SYSTEM_OUT">  
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />  
        </Console>  
    </Appenders>  
    <Loggers>  
        <Logger name="hive.server2.query.ApiQueryTest" level="error">  
            <AppenderRef ref="Console" />  
        </Logger>  
        <Logger name="hive.server2.query" level="error">  
            <AppenderRef ref="Console" />  
        </Logger>  
        <Root level="error">  
            <AppenderRef ref="Console" />  
        </Root>  
    </Loggers>  
</Configuration>  